{
  "name": "my-ecommerce-backend",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "dev": "nodemon",
    "start": "NODE_ENV=production node -r tsconfig-paths/register -r ts-node/register build/server.js",
    "build:dev": "tsc --project ./",
    "build:prod": "tsc --project ./tsconfig.prod.json && babel ./src --out-dir ./build --extensions '.ts,.js'",
    "db:fill": "yarn build && NODE_ENV=development node -r tsconfig-paths/register -r ts-node/register build/scripts/dbSeeder.js",
    "db:empty": "yarn build && NODE_ENV=development node -r tsconfig-paths/register -r ts-node/register build/scripts/dbSeeder.js -e",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "AlexisGlez",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.19",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.10",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-typescript": "^7.13.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "nodemon": "^2.0.6"
  }
}
